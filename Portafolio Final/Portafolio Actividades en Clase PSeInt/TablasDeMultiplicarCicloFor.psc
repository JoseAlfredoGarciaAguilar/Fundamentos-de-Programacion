Algoritmo TablasDeMultiplicarCicloFor
	Definir num Como Entero
	num <- 9
	Escribir '**************'
	Escribir 'Tabla de Multiplicación'
	Escribir '**************'
	Para j<-1 Hasta 10 Hacer
		Escribir num,' x ',j,' = ',num*j
	FinPara
	Escribir ' '
	Para j<-10 Hasta 1 Con Paso -1 Hacer
		Escribir num,' x ',j,' = ',num*j
	FinPara
FinAlgoritmo

