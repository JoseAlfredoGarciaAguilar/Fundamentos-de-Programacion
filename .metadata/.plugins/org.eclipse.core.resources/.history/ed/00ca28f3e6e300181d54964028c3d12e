package Portafolio;

public class VariablesTipoBoolean {

	public static void main(String[] args) {
		
		boolean bisiesto;
		bisiesto = true;
		boolean encontrado, bandera;
		encontrado = true;
		bandera = false;
		if (encontrado)
			System.out.println("Verdadero");
		else
			System.out.println("Falso");
		encontrado = (3>2) & (5<10);
		int y1 = 9;
		boolean estaEnRango;
		estaEnRango = (y1<-1) || (y1>15);
		if (estaEnRango)
			System.out.println("El valor "+y1+" está Rango");
		else
			System.out.println("El valor "+y1+" no está Rango");
	}

}