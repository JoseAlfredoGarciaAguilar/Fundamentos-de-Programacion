package Portafolio;

public class ConversiónImplícita2 {

	public static void main(String[] args) {
		long medida_milimetros;
		long distancia_media;
		long numeros_grandes = (long)(40000.5877);
		long numeros_grandes2 = 40000L;
		System.out.println(numeros_grandes);
		System.out.println(numeros_grandes2);
	}
}