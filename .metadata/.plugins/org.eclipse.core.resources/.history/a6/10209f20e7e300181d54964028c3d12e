package Portafolio;

public class VariablesTipoBoolean {

	public static void main(String[] args) {
		
		boolean bisiesto;
		bisiesto = true;
		boolean encontrado, bandera;
		encontrado = true;
		bandera = false;
		if (encontrado)
			System.out.println("Verdadero");
		else
			System.out.println("Falso");
	}
}